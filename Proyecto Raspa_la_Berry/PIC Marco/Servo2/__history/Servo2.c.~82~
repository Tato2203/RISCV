#include <16f877a.h>
#use DELAY(CLOCK=8M)
#FUSES HS, NOWDT, NOLVP
int i;


void main(){
   while(true){
   
      for(i=0;i<=100;i++) {
         output_high(pin_e0);
         output_high(pin_e1);
         output_high(pin_b3);
         output_high(pin_b2);
         output_high(pin_b1);
         delay_us(1600);
         output_low(pin_e0);
         output_low(pin_e1);
         output_low(pin_b3);
         output_low(pin_b2);
         output_low(pin_b1);
         delay_us(19000);
      }
      
      /* for(i=0;i<=100;i++) {
         output_high(pin_e0);
         output_high(pin_e1);
         output_high(pin_b3);
         output_high(pin_b2);
         output_high(pin_b1);
         delay_us(2100);
         output_low(pin_e0);
         output_low(pin_e1);
         output_low(pin_b3);
         output_low(pin_b2);
         output_low(pin_b1);
         delay_us(19000);
      }*/
   }
}
